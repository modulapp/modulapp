{
    "name": "modulapp",
    "version": "0.0.0",
    "description": "Modular application framework",
    "main": "index.js",
    "engines": {
        "node": ">=6.0.0",
        "npm": ">=3.0.0"
    },
    "scripts": {
        "test": "node node_modules/mocha/bin/mocha",
        "cover": "node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha --report lcovonly -- -R min",
