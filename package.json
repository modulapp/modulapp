        "architect",
        "dependency"
    ],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/modulapp/modulapp/issues"
    },
    "homepage": "https://github.com/modulapp/modulapp#readme",
    "devDependencies": {
        "coveralls": "^2.11.14",
        "istanbul": "^0.4.5",
        "mocha": "^3.0.2",
        "mocha-lcov-reporter": "^1.2.0",
        "should": "^11.1.0",
        "sinon": "^1.17.5"
    },
    "dependencies": {
        "async": "^2.0.1",
        "dependency-graph": "^0.5.0",
        "lodash": "^4.15.0"
