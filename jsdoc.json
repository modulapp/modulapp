{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["doc/README_content.md", "src/app.js", "src/moduleWrapper.js", "src/module.js"],
        "exclude": ["node_modules/", "test/", "website/", "resources/", "lib/"]
    },
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "opts": {
        "template": "node_modules/docdash/",
        "encoding": "utf8",
        "destination": "website/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "docdash": {
        "static": true,
        "sort": false
    }
}
